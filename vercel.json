{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api.js"
    },
    {
      "src": "/public/index.html",
      "dest": "/api.js"
    },
     {
      "src": "/public/index.html",
      "dest": "/(.*)"
    }
  ]
}
